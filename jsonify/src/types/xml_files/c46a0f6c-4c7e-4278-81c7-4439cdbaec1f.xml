                  <paragraph>
                     <content styleCode="bold">Strides</content>
                     <content styleCode="bold">Pharma Inc.</content>
                  </paragraph>
                  <paragraph>East Brunswick, NJ 08816</paragraph>
                  <paragraph>Revised: 10/21</paragraph>
               </text>
               <effectiveTime value="20220228"/>
            </section>
         </component>
