            <section ID="Lf6cb231b-ac78-400a-a5ed-95ed89c742c8">
               <id root="97f30f29-8e03-36fd-e053-2a95a90a884b"/>
               <code code="55105-1" codeSystem="2.16.840.1.113883.6.1" displayName="OTC - PURPOSE SECTION"/>
               <title>Purpose</title>
               <text>
                  <paragraph>Antiseptic</paragraph>
               </text>
