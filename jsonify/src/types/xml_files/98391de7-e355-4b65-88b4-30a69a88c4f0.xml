               <component>
                  <section>
                     <id root="6411975a-7d1e-45f3-b95c-eed5ce9792ab"/>
                     <code code="44425-7" codeSystem="2.16.840.1.113883.6.1" displayName="STORAGE AND HANDLING SECTION"/>
                     <title>Store and Dispense</title>
                     <effectiveTime value="20250115"/>
                     <component>
                        <section>
                           <id root="26700540-0b4d-4976-ab99-a497631f8f0b"/>
                           <code code="42229-5" codeSystem="2.16.840.1.113883.6.1" displayName="SPL UNCLASSIFIED SECTION"/>
                           <title>Ampules</title>
                           <text>
                              <paragraph>Store in refrigerator, 2°C-8°C (36°F-46°F). Protect from light. Administer only if solution is clear and colorless.</paragraph>
                           </text>
                           <effectiveTime value="20250115"/>
                        </section>
                     </component>
                  </section>
               </component>
            </section>
