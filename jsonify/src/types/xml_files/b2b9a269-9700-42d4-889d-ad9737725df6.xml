            <section ID="i4i_section_id_f5ae8d13-4f0f-467f-bf78-eef702284aee">
               <id root="5d53556b-4c7a-4078-9bd2-112cf64a2f0d"/>
               <code code="42229-5" codeSystem="2.16.840.1.113883.6.1" displayName="SPL UNCLASSIFIED SECTION"/>
               <text>
                  <paragraph>
                     <content styleCode="bold">For Dermatologic Use Only</content>
                  </paragraph>
                  <paragraph>
                     <content styleCode="bold">Not For Ophthalmic Use</content>
                  </paragraph>
                  <paragraph>
                     <content styleCode="bold">Rx Only</content>
                  </paragraph>
               </text>
               <effectiveTime value="20250403"/>
            </section>
