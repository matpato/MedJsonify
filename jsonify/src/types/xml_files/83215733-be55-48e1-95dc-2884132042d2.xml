               <title>PHYSICIAN PRESCRIBING INFORMATION</title>
               <text>
                  <paragraph>
                     <content styleCode="bold">Rx only</content>
                  </paragraph>
                  <paragraph>
                     <content styleCode="bold">FOR EXTERNAL USE ONLY</content>
                  </paragraph>
               </text>
               <effectiveTime value="20191224"/>
            </section>
         </component>
         <component>
            <section ID="ID_1c11b264-0113-44a8-87ee-d5e2ec655dd4">
               <id root="3716f9b8-3782-4286-982b-b6ce864d6748"/>
               <code code="42229-5" codeSystem="2.16.840.1.113883.6.1" displayName="SPL UNCLASSIFIED SECTION"/>
